{
  "name": "webpack-setup",
  "version": "0.0.1",
  "description": "Start a project from scratch",
  "scripts": {
    "setup": "mkdir -p dist && mkdir -p dist/css",
    "dev": "webpack-dev-server --progress --colors --devtool inline-source-map",
    "clean": "rm -fr dist/",
    "build": "npm run setup && webpack --progress --colors -p"
  },
  "author": "Robb Charnock",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "^2.2.0"
  }
}
